{
  "name": "nativescript-intercom-bridge",
  "version": "1.0.0",
  "description": "NativeScript Plugin for Intercom",
  "main": "intercom-bridge",
  "typings": "index.d.ts",
  "nativescript": {
    "platforms": {
      "android": "4.0.0",
      "ios": "4.0.0"
    }
  },
  "scripts": {
    "build": "npm i && tsc",
    "plugin.link": "npm link && cd ../demo && npm link nativescript-intercom-bridge && cd ../src",
    "demo.ios": "npm run preparedemo && cd ../demo && tns run ios --syncAllFiles",
    "demo.android": "npm run preparedemo && cd ../demo && tns run android",
    "test.ios": "cd demo && tns test ios --emulator",
    "test.ios.device": "cd demo && tns test ios",
    "test.android": "cd demo && tns test android",
    "preparedemo": "npm run build && cd ../demo && tns plugin remove nativescript-intercom-bridge && tns plugin add ../src && tns install",
    "setup": "npm run build && cd ../demo && npm i && tns plugin add ../src && cd ../src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aemr3/nativescript-intercom-bridge.git"
  },
  "keywords": [
    "NativeScript",
    "JavaScript",
    "Android",
    "iOS"
  ],
  "author": {
    "name": "Emre Suzen",
    "email": "e@emre.pm"
  },
  "bugs": {
    "url": "https://github.com/aemr3/nativescript-intercom-bridge/issues"
  },
  "license": "MIT",
  "homepage": "https://github.com/aemr3/nativescript-intercom-bridge",
  "readmeFilename": "README.md",
  "devDependencies": {
    "tns-core-modules": "~4.0.0",
    "tns-platform-declarations": "~4.2.1",
    "typescript": "~2.8.1",
    "prompt": "^1.0.0",
    "rimraf": "^2.5.0"
  }
}
